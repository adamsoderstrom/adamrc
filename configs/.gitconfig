[pull]
    ff=only
[user]
	name = adamsoderstrom
	email = adamsoderstrom@me.com
[alias]
	adog = log --all --decorate --oneline --graph
[init]
	defaultBranch = main
[core]
	excludesfile = /Users/adamsoderstrom/.gitignore
  pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true    # use n and N to move between diff sections
    paging = always
    syntax-theme = Kanagawa
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
[rebase]
    autosquash = true
[rerere]
    enabled = true
[http]
	postBuffer = 157286400
[branch]
    sort = -committerdate
